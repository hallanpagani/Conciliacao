@model List<ConciliacaoModelo.model.conciliador.ArquivoResumo>

@{
    ViewBag.NomeDaJanela = "Resumo";
    // ViewBag.Action = "CadastrarAgenda";
    //  ViewBag.Controller = "Agenda";
    //  ViewBag.Controller = "Agenda";
    //   ViewBag.Title = "Agenda cadastradas";
    ViewBag.IconeTopo = "fa-list";
}

<table id="datatable_tabletools_resumo" class="table table-striped table-bordered table-hover" width="100%">
    <thead>
        <tr>

            <th>
                Resumo
            </th>
            <th>
                Estabelecimento
            </th>
            <th>
                Parcela
            </th>
            <th>
                Transação
            </th>
            <th>
                Prev.Pagto
            </th>
            <th>
                Vl.Bruto
            </th>
            <th>
                Tx Comissão
            </th>
            <th>
                Comissão
            </th>
            <th>
                Valor Rejeitado
            </th>
            <th>
                Valor.Liquído
            </th>
            <th>
                Situação
            </th>
            <th>
                Produto
            </th>
            <th>
                Lógico
            </th>

        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.resumo)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.estabelecimento)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.parcela)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.transacao)
                </td>
                <td style="text-align: center">
                    @Html.DisplayFor(modelItem => item.prev_pagamento)
                </td>
                <td style="text-align: right">
                    @Html.DisplayFor(modelItem => item.valor_bruto)
                </td>
                <td style="text-align: right">
                    @Html.DisplayFor(modelItem => item.taxa_comissao)
                </td>
                <td style="text-align: right">
                    @Html.DisplayFor(modelItem => item.vl_comissao)
                </td>
                <td style="text-align: right">
                    @Html.DisplayFor(modelItem => item.vl_rejeitado)
                </td>
                <td style="text-align: right">
                    @Html.DisplayFor(modelItem => item.vl_liquido)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.situacao)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.produto)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.terminal)
                </td>
                
            </tr>

        }
    </tbody>
</table>



