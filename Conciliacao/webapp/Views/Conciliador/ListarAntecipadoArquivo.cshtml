@model List<ConciliacaoModelo.model.conciliador.ArquivoAntecipado>

@{
    ViewBag.NomeDaJanela = "Resumo";
    // ViewBag.Action = "CadastrarAgenda";
    //  ViewBag.Controller = "Agenda";
    //   ViewBag.Title = "Agenda cadastradas";
    ViewBag.IconeTopo = "fa-list";
}

<table id="datatable_tabletools_antecipado" class="table table-striped table-bordered table-hover" width="100%">
    <thead>
        <tr>

            <th>
                Registro
            </th>
            <th>
                Estabelecimento
            </th>
            <th>
                RO
            </th>
            <th>
                Dt.Crédito
            </th>
            <th>
                Sinal Valor Bruto
            </th>
            <th>
                Valor Bruto
            </th>
            <th>
                Sinal Valor Bruto Parcelado
            </th>
            <th>
                Vl.Bruto Parcelado
            </th>
            <th>
                Sinal Valor Bruto Pré-Datado
            </th>
            <th>
                Valor Bruto Pré-Datado
            </th>
            <th>
                Sinal Valor Bruto Total
            </th>
            <th>
                Valor Bruto Total
            </th>
            <th>
                Sinal Valor Líquido
            </th>
            <th>
                Valor Líquido
            </th>
            <th>
                Sinal Valor Líquido Parcelado
            </th>
            <th>
                Valor Líquido Parcelado
            </th>
            <th>
                Valor Líquido Parcelado Pré-Datado
            </th>
            <th>
                Valor Líquido Pré-Datado
            </th>
            <th>
                Sinal Valor Líquido Total
            </th>
            <th>
                Valor Líquido Total
            </th>
            <th>
                Tx.Desconto
            </th>

            <th>
                Banco
            </th>
            <th>
                Agência
            </th>
            <th>
                Conta corrente
            </th>

            <th>
                Sinal Valor Liquído Antecipação
            </th>
            <th>
                Valor Liquído Antecipação
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.registro)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.estabelecimento)
                </td>

                <td>
                    @Html.DisplayFor(modelItem => item.num_resumo)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.dt_credito)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.sinal_valor_bruto)
                </td>


                <td>
                    @Html.DisplayFor(modelItem => item.valor_bruto)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.sinal_valor_bruto_parcelado)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.valor_bruto_parcelado)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.sinal_valor_bruto_predatado)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.valor_bruto_predatado)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.sinal_valor_bruto_total)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.valor_bruto_total)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.sinal_valor_liquido)
                </td>

                <td>
                    @Html.DisplayFor(modelItem => item.valor_liquido)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.sinal_valor_liquido_parcelado)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.valor_liquido_parcelado)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.sinal_valor_liquido_predatado)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.valor_liquido_predatado)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.sinal_valor_liquido_total)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.valor_liquido_total)
                </td>

                <td>
                    @Html.DisplayFor(modelItem => item.taxa_desconto)
                </td>

                <td>
                    @Html.DisplayFor(modelItem => item.banco)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.agencia)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.conta_corrente)
                </td>

                <td>
                    @Html.DisplayFor(modelItem => item.sinal_valor_liquido_antecipacao)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.valor_liquido_antecipacao)
                </td>

            </tr>

        }
    </tbody>
</table>




