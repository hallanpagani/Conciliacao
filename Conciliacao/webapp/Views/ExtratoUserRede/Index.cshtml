@section pagespecific {



    <script src="~/scripts/terceiros/data.js" type="text/javascript"></script>

    <script type="text/javascript">

        var reader; //GLOBAL File Reader object for demo purpose only

        function FormataData(data) {
            return data.substr(0, 2) + '/' + data.substr(2, 2) + '/' + data.substr(4, 4);
        }

        Array.prototype.sum = function (prop) {
            var total = 0
            for (var i = 0, _len = this.length; i < _len; i++) {
                total += this[i][prop]
            }
            return total
        }

        Number.prototype.format = function (n, x, s, c) {
            var re = '\\d(?=(\\d{' + (x || 3) + '})+' + (n > 0 ? '\\D' : '$') + ')',
                num = this.toFixed(Math.max(0, ~~n));

            return (c ? num.replace('.', c) : num).replace(new RegExp(re, 'g'), '$&' + (s || ','));
        };


        /**
         * read text input
         */
        function readText(filePath) {
            var output = ""; //placeholder for text output
            reader = new FileReader();
            if (filePath.files && filePath.files[0]) {
                reader.onload = function (e) {
                    output = e.target.result;
                    if (filePath.files[0].name.indexOf('EEVD') !== -1) {
                        displayContentsEEVD(output);
                    } else if (filePath.files[0].name.indexOf('EEVC') !== -1) {
                        displayContentsEEVC(output);
                    } else {
                        displayContentsEEFI(output);
                    }

                }; //end onload()
                reader.readAsText(filePath.files[0]);
            } //end if html5 filelist support
            else if (ActiveXObject && filePath) { //fallback to IE 6-8 support via ActiveX
                try {
                    reader = new ActiveXObject("Scripting.FileSystemObject");
                    var file = reader.OpenTextFile(filePath, 1); //ActiveX File Object
                    output = file.ReadAll(); //text contents of file
                    file.Close(); //close file "input stream"
                    displayContents(output);
                } catch (e) {
                    if (e.number == -2146827859) {
                        alert('Unable to access local files due to browser security settings. ' +
                            'To overcome this, go to Tools->Internet Options->Security->Custom Level. ' +
                            'Find the setting for "Initialize and script ActiveX controls not marked as safe" and change it to "Enable" or "Prompt"');
                    }
                }
            } else { //this is where you could fallback to Java Applet, Flash or similar
                return false;
            }
            return true;
        }

        /**
         * display content using a basic HTML replacement
         */
        function displayContentsEEVD(txt) {
            var res = txt.substring(0, 186).split(",");

            document.getElementById('tp_arquivo').value = res[4];
            document.getElementById('datafinal').value = res[3].substr(0, 2) + '/' + res[3].substr(2, 2) + '/' + res[3].substr(4, 4);
            document.getElementById('datainicial').value = res[2].substr(0, 2) + '/' + res[2].substr(2, 2) + '/' + res[2].substr(4, 4);
        }

        function displayContentsEEVC(txt) {

            document.getElementById('tp_arquivo').value = txt.substr(19, 30) + "- EEVC";
            document.getElementById('datainicial').value = txt.substr(3, 2) + '/' + txt.substr(5, 2) + '/' + txt.substr(7, 4);
            document.getElementById('datafinal').value = txt.substr(3, 2) + '/' + txt.substr(5, 2) + '/' + txt.substr(7, 4);
        }

        function displayContentsEEFI(txt) {

            document.getElementById('tp_arquivo').value = txt.substr(19, 30) + "- EEFI";
            document.getElementById('datainicial').value = txt.substr(3, 2) + '/' + txt.substr(5, 2) + '/' + txt.substr(7, 4);
            document.getElementById('datafinal').value = txt.substr(3, 2) + '/' + txt.substr(5, 2) + '/' + txt.substr(7, 4);
        }

        var mydata;

        function Processando(this1) {

            this1.disabled = true;
            this1.innerHTML = '<i class="fa fa-refresh fa-spin"></i> Processando…';
            // evt.preventDefault();
            var adata = new FormData();

            var files = $("#arquivo").get(0).files;

            //Add the uploaded image content to the form data collection
           
            for (var x = 0; x < files.length; x++) {
                adata.append("Arquivo" + x, files[x]);
            }


           /* if (files.length > 0) {
                adata.append("Arquivo", files);
            } */

            if (files[0].name.indexOf('EEVD') !== -1) {

                $("#loadMe").modal({
                    backdrop: 'static', //remove ability to close modal with click
                    keyboard: false, //remove option to close with keyboard
                    show: true //Display loader!
                });

                // Make Ajax request with the contentType = false, and procesDate = false
                var ajaxRequest = $.ajax({
                    type: "POST",
                    url: '@Url.Action("ProcessarArquivo", "ExtratoUserRede")',
                    contentType: false,
                    processData: false,
                    data: adata,
                    error: function (error) {
                        alert(error);
                        console.log(error);
                    },
                    success: function (jsondata) {

                        arquivoresumo = jsondata.ArquivosDeCartoesEEVDUserRede;

                        var table_resumo = $('#datatable_tabletools_resumo').dataTable({
                            scrollX: true,
                            destroy: true,
                            "columns": [
                                { "data": "is_numero_resumo_venda" },
                                { "data": "nm_tipo_registro" },
                                { "data": "is_numero_filiacao_pv" },
                                { "data": "data_credito" },
                                { "data": "data_resumo_venda" },
                                { "data": "is_numero_resumo_venda" },
                                { "data": "is_quantidade_resumo_vendas" },
                                { "data": "is_valor_bruto" },
                                { "data": "is_valor_desconto" },
                                { "data": "is_valor_liquido" },
                                { "data": "is_tipo_resumo" },
                                { "data": "is_banco" },
                                { "data": "is_agencia" },
                                { "data": "is_conta_corrente" },
                                { "data": "is_bandeira" }
                            ],
                            "columnDefs": [
                                {
                                    "targets": 0,
                                    "orderable": false,
                                    "bSearchable": false,
                                    "data": "is_numero_resumo_venda",
                                    "render": function (data, type, full, meta) {
                                        return '<button type="button" data-resumo="' + data + '" onClick="DetalhesEEVD(this);" class="btn btn-primary editar_resumo">Detalhes</button>';
                                    }
                                },
                                { className: "valortd", "targets": [7, 8, 9] }
                            ],

                            aaData: arquivoresumo,

                            language: {
